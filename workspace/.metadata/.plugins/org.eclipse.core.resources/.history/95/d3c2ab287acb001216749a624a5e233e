package nl.vu.cs.cn;

/**
 * @author Steven van Beelen, 1821806
 * 
 * An enumeration class for all the possible state
 * 	a TCP connection can be in.
 */
public enum ConnectionState {
	CLOSED, LISTEN, 
	SYN_SENT, SYN_RCVD, 
	ESTABLISHED, 
	FIN_WAIT_1, FIN_WAIT_2, 
	CLOSE_WAIT, CLOSING, 
	LAST_ACK, TIME_WAIT;
}